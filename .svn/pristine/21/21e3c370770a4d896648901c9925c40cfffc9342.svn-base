.down1{
	position: absolute;
	float: left;
    width: 17.5%;
    height: 175px;
    left: 6px;
    background-color: white;
    overflow: auto;
}
.down1::-webkit-scrollbar {/*滚动条整体样式*/
        width: 5px;     /*高宽分别对应横竖滚动条的尺寸*/
        height: 1px;
    }
.down1::-webkit-scrollbar-thumb {/*滚动条里面小方块*/
        border-radius: 10px;
         -webkit-box-shadow: inset 0 0 5px rgba(0,0,0,0.2);
        background:#a6a6a6;
    }
.down1::-webkit-scrollbar-track {/*滚动条里面轨道*/
        -webkit-box-shadow: inset 0 0 5px rgba(0,0,0,0.2);
        border-radius: 10px;
        background: #EDEDED;
    }


.down1 ul{
	margin-top: 8px;
    width: 170px;
    margin-left: 8px;
    border-radius: 8px;
    text-align: center;
}
.down1 ul li{
	height: 22px;
    margin-top: 5px;
    border-radius: 8px;
    background-color: rgba(12, 12, 12, 0.18);
}
.down1 ul li:hover{
	background-color: #3F3F3F;
}

