<script>
    /* 导航栏下拉列表 */
    layui.use('element', function(){
        var element = layui.element;
    });

    /* 导航栏点击事件*/
    function selSecond(obj){
        var navbar = $('#navbar-sup dd');
        for(var i=0;i<navbar.length;i++){
            if(navbar[i]!=obj){
                $(navbar[i]).css('background-color','#000000');
            }else{
                $(navbar[i]).css('background-color','#6E0000');
            }
        }
    }

    /* 菜单栏点击事件 */
    function sel(obj) {
        var menu = $(".menulist li")
        for(var i=0;i<menu.length;i++) {
            if(menu[i]!=obj) {
                $(menu[i]).css('background-color','#000000')
            }
            else{
                $(menu[i]).css('background-color','#b7b7b7')
            }
        }

        }

        /* 仪表盘切换 */
    $("#pro").click(function () {
        $("#pro-tab").css("display","inline-table");
        $("#bud-tab").css("display","none");
        $("#projectper").css("display","block");
        $("#budget").css("display","none");
    });
    $("#bud").click(function () {
        $("#pro-tab").css("display","none");
        $("#bud-tab").css("display","inline-table");
        $("#projectper").css("display","none");
        $("#budget").css("display","block");
    })


    /* FP资源管理弹出层 */
    function iframefp(){
        layer.open({
            type: 2
            ,title: 'FP资源管理'
            ,maxmin: true
            ,scrollbar: false
            ,shade: [0.4,'#000000']
            ,area: ['71%','80%']
            ,content: 'fp-resouce.html'
        });
    }
    /* 日志信息弹出层 */
    function iframelog(){
        layer.open({
            type: 2
            ,title: '日志管理'
            ,maxmin: true
            ,scrollbar: false
            ,shade: [0.4,'#000000']
            ,area: ['70%','80%']
            ,content: 'loginfo.html'
        });
    }
    /* 改密弹出层 */
    function iframeedit(){
        layer.open({
            type: 2
            ,title: '修改密码'
            ,maxmin: true
            ,scrollbar: false
            ,shade: [0.4,'#000000']
            ,area: ['70%','50%']
            ,content: 'edit.html'
        });
    }
    /* 权限弹出层 */
    function iframerole(){
        layer.open({
            type: 2
